<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.android10.viewgroupperformance" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <android:uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <android:uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/myAppTheme" >
        <activity
            android:name=".activity.MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.FeedActivity"
            android:screenOrientation="portrait"
            android:label="@string/relative_layout_test" />
        <activity
            android:name=".activity.RelativeLayoutTestActivity"
            android:label="@string/relative_layout_test" />
        <activity
            android:name=".activity.LinearLayoutTestActivity"
            android:label="@string/linear_layout_test" />
        <activity
            android:name=".activity.FrameLayoutTestActivity"
            android:label="@string/frame_layout_test" />
        <activity
            android:name=".activity.ProfileActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_profile" >
        </activity>
    </application>

</manifest>
